/*Topo*/

.header-logo, .checkout-header-logo{
	background-image: url("../imagens/@TemplateSettings.header_logo_image") !important;
}

.back_logo{
		background-image: url("../imagens/@TemplateSettings.header_fundo_logo_image") !important;

}

/* Checkout Header */

.step-identification .checkout-header-step{ 
    background: url('../imagens/@TemplateSettings.checkoutHeader_step_identification_image') no-repeat 0 0 scroll transparent;
}

.step-shipping .checkout-header-step, .step-gift .checkout-header-step{ 
    background: url('../imagens/@TemplateSettings.checkoutHeader_step_delivery_image') no-repeat 0 0 scroll transparent;
}

.step-payment .checkout-header-step{
    background: url('../imagens/@TemplateSettings.checkoutHeader_step_payment_image') no-repeat 0 0 scroll transparent;
}


