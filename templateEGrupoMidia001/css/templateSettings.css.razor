@if(TemplateSettings.collections_quickView_enabled!="true"){
	<text>
	.collection-product-quickView{display: none!important;}	
	</text>
}
@if(TemplateSettings.collections_buy_enabled!="true"){
	<text>
	.collection-product-buy{display: none!important;}
    </text>
}
.collection-product-buy a, .news a, #condicaobotao_comprar_direto a,  .collection-ordenation label,
 .nav-PriceRange a,.buy-message-bt a, .heading-secundary h2, .heading-secundary h1, .product-descriptions-titles li{
	color:@TemplateSettings.butonsfontColor;

}

/*Listas Prontas*/

.quickLists-home{background-image: url("../imagens/@TemplateSettings.quickLists_homeImage_backgroundImage")}
@if(TemplateSettings.quickLists_show != "true"){
	<text>
		.quickList-show{ display: none; }
	</text>
}

/* Lista de presentes */
@if(TemplateSettings.giftList_show != "true"){
