<div class="box" ng-app="AppAngular" ng-controller="bundle">
    <div class="heading-special">
        <h2>Compre Junto</h2>
    </div> 
    <ul class="compre_junto">      
        <li ng-repeat="bundle in bundles">
            <div class="fotos_info">
              <div class="fotos">
                  <div class="compre_junto_foto foto_principal">                        
                        <div class="compre_junto_foto_foto">
                           <img src="{{ bundle.product.selectedVariant.mainImage }}?width=100&height=100" alt="{{ bundle.product.selectedVariant.name }}" border="0" />
                        </div>
                        <div class="compre_junto_selecionar">
                            <img src="imagens/compre_junto_selecionado.png" alt="" /> <span>Selecionado</span>
                        </div>
                        <div class="compre_junto_foto_variantes">
                          &nbsp;
                        </div>
                        <div class="compre_junto_foto_quantidade">
                            <span class="qtd">Quantidade</span>
                            <img src="imagens/compre_junto_remove.png" alt="" class="comnpre_junto_remove" />
                            <input type="text" ng-model="product.quantity" maxlength="3" />
                            <img src="imagens/compre_junto_adiciona.png" alt="" class="comnpre_junto_adiciona" />
                        </div>
                  </div>
                  <span data-ng-repeat="item in bundle.products" class="compre_junto_itens">
                    <div class="compre_junto_mais">
                        <img src="imagens/compre_junto_item_mais.png?19" alt="Mais" />
                    </div>
                    <div class="compre_junto_foto">                        
                        <div class="compre_junto_foto_foto">
                            <a href="{{item.url}}"><img src="{{item.selectedVariant.mainImage}}?width=100&height=100" alt="{{item.name}}" border="0" /></a>
                        </div>
                        <div class="compre_junto_selecionar">
                          <label><input type="checkbox" ng-model="item.selected" /><span>Selecionado</span></label>
                        </div>
                        <div class="compre_junto_foto_variantes">
                          <select ng-if="item.variants.length > 1" ng-model="item.variant" ng-change="item.changeVariant(item, item.variant)" ng-options="s.definition1Value + ' ' + s.definition2Value for s in item.variants | orderBy: ['definition1Value', 'definition2Value']">
                          </select>                       
                        </div>
                        <div class="compre_junto_foto_quantidade">
                            <span class="qtd">Quantidade</span>
                            <img src="imagens/compre_junto_remove.png" alt="" class="comnpre_junto_remove" ng-if="item.selected == true" />
                            <input type="text" ng-model="item.quantity" ng-disabled="item.selected == false" class="numero" maxlength="3" />
                            <img src="imagens/compre_junto_adiciona.png" alt="" class="comnpre_junto_adiciona" ng-if="item.selected == true" />
                        </div>
                    </div>
                  </span>
              </div>
              <div class="infos">
                  <p class="nome"><strong><span class="mais">&rsaquo;</span>{{product.name}}</strong>
                    <span>
                        <a href="{{item.url}}" data-ng-repeat="item in bundle.products"><span class="mais">+</span> {{item.selectedVariant.name}}</a>
                    </span>
                  </p>
                  <div class="preco_comprar">
                      <p class="preco">Por: <strong>{{bundle.adjustedTotalPrice | currency:"R$"}}</strong></p>
                      <p class="no_boleto" ng-if="bundle.payInFullTotalPrice != bundle.adjustedPrice">ou <strong>{{bundle.payInFullTotalPrice | currency:"R$"}}</strong> no boleto.</p>
                      <p class="economize" ng-if="bundle.totalPrice != bundle.adjustedTotalPrice">Economize: <strong>{{bundle.totalPrice - bundle.adjustedTotalPrice | currency:"R$"}}</strong></p>
                      <div class="bt">
                            <div>
                                <a href="javascript:void(0);" class="bt-compre-junto btn-primary" alt="Comprar os dois" border="0" ng-click="bundle.addToCart()" style="cursor: pointer"><span>Comprar</span> todos os produtos</a> 
                            </div>
                      </div>
                  </div>
              </div>
            </div>
 
        </li>
    </ul>
</div>
 
<script type="text/javascript">
 
$(function(){
    $(".comnpre_junto_remove").live("click",function(){
        var input = $(this).parents(".compre_junto_foto_quantidade").children("input");      
        if(input.prop( "disabled", false )){      
          var qtd = parseInt(input.attr("value"));
          if(input.attr("value")>1){
            input.attr("value", qtd-1); 
            input.trigger("change");
          };
        };
    });
 
    $(".comnpre_junto_adiciona").live("click",function(){
        var input = $(this).parents(".compre_junto_foto_quantidade").children("input");      
        if(input.prop( "disabled", false )){      
          var qtd = parseInt(input.attr("value"));
          input.attr("value", qtd+1);
          input.trigger("change");  
        };
    });
 
});
 
  $(function(){
      $('.numero').live('keydown',soNums); 
  });
 
  function soNums(e){
 
      keyCodesPermitidos = new Array(8,9,37,39,46);
 
      for(x=48;x<=57;x++){
          keyCodesPermitidos.push(x);
      }
 
      for(x=96;x<=105;x++){
          keyCodesPermitidos.push(x);
      }
 
      keyCode = e.which;
 
      if ($.inArray(keyCode,keyCodesPermitidos) != -1){
          return true;
      }   
      return false;
  }   
 
</script>
 
}]]></BundlesHTML></Bundles></Bundless></controles></page>