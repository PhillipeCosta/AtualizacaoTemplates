          var precoPor = $('.vitrinePrecoPor', this).text(); 
          var precoDe2 = parseFloat(precoDe.slice(2).replace('.','').replace(',','.')).toFixed(2); 
          var precoPorAjuste = precoPor.replace(/(\s|\n)+/g," ").replace(/^\s|\s$/g,""); 
