          var valorDescontoDiv = parseFloat(diferenca*100).toFixed(2);
          var valorDesconto = parseInt(valorDescontoDiv/precoDe2).toFixed(0);
          $('.ofertaLabelProd', this).html('<div class="etiquetaValorDesconto"><div class="boxOfertaQ">-'+valorDesconto+'%</div><div class="boxOfertaT"></div></div>');
