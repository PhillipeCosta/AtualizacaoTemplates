<script>
  
function printDiv(id, pg) {
    var oPrint, oJan;
    oPrint = window.document.getElementById(id).innerHTML;
    oJan = window.open(pg,'_blank','width=300,height=400');
    oJan.document.write(oPrint);
    oJan.focus();
    oJan.window.print();
    window.onafterprint(setTimeout("oJan.document.close()",50));
}
    
</script>]]></ListaPresentesCadastroHTML><MensagemAlertaListaPresentesCadastroHTML><![CDATA[<div class="alert alert-warning" role="alert" id="erro">
