/* Main */

.main {
  background-color: @TemplateSettings.main_backgroundColor;
  
  @if(TemplateSettings.main_backgroundImage_display == "true"){
      <text>
          background-image: url(../imagens/@TemplateSettings.main_backgroundImage);
      </text>
  }
  @if(TemplateSettings.main_backgroundImage_fixed == ""){
    <text>
        background-attachment: scroll;
      </text>
  }
  @if(TemplateSettings.main_backgroundImage_fixed == "true"){
    <text>
          background-attachment: fixed;
      </text>
  }
  @if(TemplateSettings.main_backgroundImage_position == "center"){
    <text>
      background-position: center 0px;
      </text>
  }
  @if(TemplateSettings.main_backgroundImage_position == "left"){
    <text>
          background-attachment: left 0px;
      </text>
  }
  @if(TemplateSettings.main_backgroundImage_position == "right"){
    <text>
          background-attachment: right 0px;
      </text>
  }
  @if(TemplateSettings.main_backgroundImage_repeat == ""){
    <text>
        background-repeat: no-repeat;
      </text>
  }
  @if(TemplateSettings.main_backgroundImage_repeat == "true"){
    <text>
          background-repeat: repeat;
      </text>
  }
  
  color: @TemplateSettings.main_color;
  font-size: @TemplateSettings.main_fontSize;
}

.main-container, .conteudo_fixo{
  background: @TemplateSettings.main_container_backgroundColor;
}

a{ 
